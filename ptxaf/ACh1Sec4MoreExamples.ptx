  <exercise xml:id="quiz_different_functions">
    <statement>
      <p>
        Laat <m>X = \{a, b, c\}</m>.

        <ol>
          <li xml:id="three_function_question">
            <p>
              Skryf drie verskillende funksies <m>\mathbf{f}, \mathbf{g}, \mathbf{h}</m> in <m>\Fun(X)</m> neer.
            </p>
          </li>

          <li>
            <p>
              Vir die funksies wat jy in
              <xref ref="three_function_question" text="global"/> geskryf het, bereken <m>\mathbf{f} + \mathbf{g}</m>,
              asook <m>3 \smul \mathbf{h}</m>.
            </p>
          </li>
        </ol>
      </p>
    </statement>
      <solution>
        <p>
        <ol>
          <li>
            <md>
            <mrow> f(a)= 4 </mrow>
            <mrow> f(b) = 0 </mrow>
            <mrow> f(c) = 2 </mrow>
            <mrow></mrow>
            <mrow> g(a)= 1 </mrow>
            <mrow> g(b) = 1 </mrow>
            <mrow> g(c) = 1 </mrow>
            <mrow></mrow>
            <mrow> h(a)= 0 </mrow>
            <mrow> h(b) = 3</mrow>
            <mrow> h(c) = 0 </mrow>
          </md>
          </li>
          <li>
          <md>
          <mrow> (f+g)(a) =5</mrow>
          <mrow> (f+g)(b) =1 </mrow>
          <mrow> (f+g)(c) = 3</mrow>
          <mrow></mrow>
          <mrow>( 3 \smul h)(a) = 0 </mrow>
          <mrow>( 3 \smul h)(b) = 9 </mrow>
          <mrow>( 3 \smul h)(c) = 0 </mrow>
          </md>
          </li>
        </ol>
        </p>
      </solution>
  </exercise>

